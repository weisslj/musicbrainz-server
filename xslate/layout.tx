<!DOCTYPE html>
<html>
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <title><: l('MusicBrainz - The Open Music Encyclopedia') :></title>
    <link rel="search" type="application/opensearchdescription+xml"
          title="<: l('MusicBrainz: Artist') :>"
          href="<: $c.uri_for('/static/search_plugins/opensearch/musicbrainz_artist.xml') :>" />

    <: css_manifest('common.css.manifest') :>

    <link type="text/css" rel="stylesheet" href="/static/lib/jquery.ui/themes/base/jquery.ui.all.css">
    <link rel="search" type="application/opensearchdescription+xml" title="MusicBrainz: Artist"
          href="/static/search_plugins/opensearch/musicbrainz_artist.xml">
    <link rel="search" type="application/opensearchdescription+xml" title="MusicBrainz: Label"
          href="/static/search_plugins/opensearch/musicbrainz_label.xml">
    <link rel="search" type="application/opensearchdescription+xml" title="MusicBrainz: Release"
          href="/static/search_plugins/opensearch/musicbrainz_release.xml">
    <link rel="search" type="application/opensearchdescription+xml" title="MusicBrainz: Track"
          href="/static/search_plugins/opensearch/musicbrainz_track.xml">

    <script type="text/javascript">//<![CDATA[
      document.cookie = "javascript=true; path=/";
    //]]></script>

    <: script_manifest('common.js.manifest') :>
    <script type="text/javascript" src="<: $c.uri_for('/text.js') :>"></script>
    <script type="text/javascript">//<![CDATA[
      $(document).ready (function () {
        $("[placeholder]").mb_placeholder ();
        MB.Control.RelateTo ();
      });
    //]]></script>
  </head>

  <body>
    <div id="header">
      <div id="header-logo">
        <a href="/" class="logo" title="MusicBrainz"><strong>MusicBrainz</strong></a>
        <div>
          <form action="<: $c.uri_for('/search') :>" method="get">
            <input class="" id="headerid-query" name="query" placeholder="search" type="text" value="" />
            <select id="headerid-type" name="type"><option value="artist">Artist</option><option value="release_group">Release Group</option><option value="release">Release</option><option value="recording">Recording</option><option value="work">Work</option><option value="label">Label</option><option value="annotation">Annotation</option><option value="cdstub">CD Stub</option><option value="editor">Editor</option><option value="freedb">FreeDB</option><option value="tag">Tag</option></select>

            <span class="buttons header-search">
              <button type="submit">Search</button>
            </span>
          </form>
        </div>
      </div>
      <div id="header-menu">
        <div>
          <ul class="r">
            : if $c.user_exists {
              <li class="account">
                <: link_entity($c.user) :>
                  <ul>
                    <li>
                      <a href="<: $c.uri_for('/account/edit') :>"><: l('Edit Profile') :></a>
                    </li>
                    <li>
                      <a href="<: $c.uri_for_action('/account/change_password') :>"><: l('Change Password') :></a>
                    </li>
                    <li>
                      <a href="<: $c.uri_for_action('/account/preferences') :>"><: l('Preferences') :></a>
                    </li>
                    <li>
                      <a href="<: $c.uri_for_action('/user/subscriptions/artist', [ $c.user.name ]) :>"><: l('Subscriptions') :></a>
                    </li>
                    <li>
                      <a href="<: $c.uri_for_action('/user/logout') :>"><: l('Log Out') :></a>
                    </li>
                  </ul>
              </li>
              <li class="data">
                <a href="<: $c.uri_for_action('/user/profile', [ $c.user.name ]) :>"><: l('My Data') :></a>
                <ul>
                  <li>
                    <a href="<: $c.uri_for_action('/user/collections', [ $c.user.name ]) :>"><: l('My Collections') :></a>
                  </li>
                  <li>
                    <a href="<: $c.uri_for_action('/user/ratings', [ $c.user.name ]) :>"><: l('My Ratings') :></a>
                  </li>
                  <li>
                    <a href="<: $c.uri_for_action('/user/tags', [ $c.user.name ]) :>"><: l('My Tags') :></a>
                  </li>
                  <li class="separator">
                    <a href="<: $c.uri_for_action('/user/edits/open', [ $c.user.name ]) :>"><: l('My Open Edits') :></a>
                  </li>
                  <li>
                    <a href="<: $c.uri_for_action('/user/edits/all', [ $c.user.name ]) :>"><: l('All My Edits') :></a>
                  </li>
                  <li>
                    <a href="<: $c.uri_for_action('/edit/subscribed') :>"><: l('Edits for Subscribed Entities') :></a>
                  </li>
                  <li>
                    <a href="<: $c.uri_for_action('/edit/subscribed_editors') :>"><: l('Edits by Subscribed Editors') :></a>
                  </li>
                </ul>
              </li>
              : if $c.user_exists && $c.user.is_admin {
              <li class="admin">
                <a href="<: $c.uri_for_action('/admin/index') :>"><: l('Admin') :></a>
                <ul>
                  : if $c.user.is_relationship_editor {
                  <li>
                    <a href="<: $c.uri_for_action('/admin/linktype/index') :>"><: l('Edit Relationship Types') :></a>
                  </li>
                  <li>
                    <a href="<: $c.uri_for_action('/admin/linkattributetype/index') :>"><: l('Edit Relationship Attributes') :></a>
                  </li>
                  : }
                  : if $c.user.is_wiki_transcluder {
                  <li>
                    <a href="<: $c.uri_for_action('/admin/wikidoc/index') :>"><: l('Transclude WikiDocs') :></a>
                  </li>
                  : }
                </ul>
              </li>
              : }
              : }
              : else {
              <li>
                <a href="<: $c.uri_for_action('/user/login', { uri => $c.req.query_params.uri ||
                         $c.relative_uri }) :>"><: l('Log In') :></a>
              </li>
              <li><a href="<: $c.uri_for_action('/account/register') :>"><: l('Create Account') :></a></li>
              : }
          </ul>

          <ul>
            <li class="about">
              <a href="<: doc_link('About_MusicBrainz') :>"><: l('About') :></a>
              <ul>
                <li>
                  <a href="<: doc_link('MusicBrainz_Sponsors') :>"><: l('Sponsors') :></a>
                </li>
                <li>
                  <a href="<: doc_link('People') :>"><: l('People') :></a>
                </li>
                <li class="separator">
                  <a href="<: doc_link('MusicBrainz_License') :>"><: l('Data Licenses') :></a>
                </li>
                <li>
                  <a href="<: doc_link('Social_Contract') :>"><: l('Social Contract') :></a>
                </li>
                <li>
                  <a href="<: doc_link('Code_of_Conduct') :>"><: l('Code of Conduct') :></a>
                </li>
                <li>
                  <a href="<: doc_link('MusicBrainz_Privacy_Policy') :>"><: l('Privacy Policy') :></a>
                </li>
                <li class="separator">
                  <a href="<: $c.uri_for_action('/elections/index') :>"><: l('Auto-editor Elections') :></a>
                </li>
                <li>
                  <a href="<: $c.uri_for('/privileged') :>"><: l('Privileged User Accounts') :></a>
                </li>
                <li>
                  <a href="<: $c.uri_for('/statistics') :>"><: l('Statistics') :></a>
                </li>
                <li>
                  <a href="<: $c.uri_for('/statistics/timeline') :>"><: l('Timeline Graph') :></a>
                </li>
              </ul>
            </li>
            <li class="blog">
              <a href="http://blog.musicbrainz.org" class="internal">
                <: l('Blog') :>
              </a>
            </li>

            <li class="products">
              <a href="<: doc_link('Products') :>"><:  l('Products') :></a>
              <ul>
                <li>
                  <a href="<: doc_link('MusicBrainz_Picard') :>"><: l('MusicBrainz Picard') :></a>
                </li>
                <li>
                  <a href="<: doc_link('Jaikoz') :>"><: l('Jaikoz') :></a>
                </li>
                <li>
                  <a href="<: doc_link('Magic_MP3_Tagger') :>"><: l('Magic MP3 Tagger') :></a>
                </li>
                <li class="separator">
                  <a href="<: doc_link('MusicBrainz_for_Android') :>"><: l('MusicBrainz for Android') :></a>
                </li>
                <li class="separator">
                  <a href="<: doc_link('MusicBrainz_Server') :>"><: l('MusicBrainz Server') :></a>
                </li>
                <li>
                  <a href="<: doc_link('MusicBrainz_Database') :>"><: l('MusicBrainz Database') :></a>
                </li>
                <li class="separator">
                  <a href="<: doc_link('Developer_Resources') :>"><: l('Developer Resources') :></a>
                </li>
                <li>
                  <a href="<: doc_link('XML_Web_Service') :>"><: l('XML Web Service') :></a>
                </li>
                <li>
                  <a href="<: doc_link('Live_Data_Feed') :>"><: l('Live Data Feed') :></a>
                </li>
                <li class="separator">
                  <a href="<: doc_link('FreeDB_Gateway') :>"><: l('FreeDB Gateway') :></a>
                </li>
              </ul>
            </li>
            <li class="search">
              <a href="<: $c.uri_for('/search') :>"><: l('Search') :></a>
              <ul>
                : if $c.user_exists {
                <li>
                  <a href="<: $c.uri_for_action('/edit/search') :>"><: l('Search Edits') :></a>
                </li>
                : }
                <li>
                  <a href="<: $c.uri_for('/browse') :>"><: l('Browse') :></a>
                </li>
                <li>
                  <a href="<: $c.uri_for('/tags') :>"><: l('Tags') :></a>
                </li>
                <li>
                  <a href="<: $c.uri_for('/cdstub/browse') :>"><: l('Top CD Stubs') :></a>
                </li>
              </ul>
            </li>
            : if $c.user_exists {
            <li class="editing">
              <a href="<: doc_link('How_Editing_Works') :>"><: l('Editing') :></a>
              <ul>
                <li>
                  <a href="<: $c.uri_for('/artist/create') :>"><: l('Add Artist') :></a>
                </li>
                <li>
                  <a href="<: $c.uri_for('/label/create') :>"><: l('Add Label') :></a>
                </li>
                <li>
                  <a href="<: $c.uri_for_action('/release_group/create') :>"><: l('Add Release Group') :></a>
                </li>
                <li>
                  <a href="<: $c.uri_for_action('/release_editor/add') :>"><: l('Add Release') :></a>
                </li>
                <li>
                  <a href="<: $c.uri_for_action('/release_editor/add', { artist => $various_artist_mbid }) :>"><: l('Add Various Artists Release') :></a>
                </li>
                <li>
                  <a href="<: $c.uri_for_action('/recording/create') :>"><: l('Add Standalone Recording') :></a>
                </li>
                <li>
                  <a href="<: $c.uri_for_action('/work/create') :>"><: l('Add Work') :></a>
                </li>
                <li class="separator">
                  <a href="<: $c.uri_for('/edit/open') :>"><: l('Vote on Edits') :></a>
                </li>
                <li>
                  <a href="<: $c.uri_for_action('/report/index') :>"><: l('Reports') :></a>
                </li>
              </ul>
            </li>
            : }
            <li class="documentation">
              <a href="<: doc_link('MusicBrainz_Documentation') :>"><: l('Documentation') :></a>
              <ul>
                <li>
                  <a href="<: doc_link('Beginners_Guide') :>"><: l('Beginners Guide') :></a>
                </li>
                <li>
                  <a href="<: doc_link('Style') :>"><: l('Style Guidelines') :></a>
                </li>
                <li>
                  <a href="<: doc_link('Frequently_Asked_Questions') :>"><: l('FAQs') :></a>
                </li>
                <li class="separator">
                  <a href="<: $c.uri_for_action('/edit/edit_types') :>"><: l('Edit Types') :></a>
                </li>
                <li>
                  <a href="<: $c.uri_for('/admin/linktype') :>"><: l('Relationship Types') :></a>
                </li>
                <li>
                  <a href="<: $c.uri_for('/admin/linkattributetype') :>"><: l('Relationship Attributes') :></a>
                </li>
                <li>
                  <a href="<: $c.uri_for('/admin/linkattributetype/instruments') :>"><: l('Instrument Tree') :></a>
                </li>
                <li class="separator">
                  <a href="<: doc_link('Development') :>"><: l('Development') :></a>
                </li>
              </ul>
            </li>
            <li class="contact">
              <a href="<: doc_link('Contact_Us') :>"><: l('Contact Us') :></a>
              <ul>
                <li>
                  <a href="<: doc_link('Mailing_Lists') :>"><: l('Mailing Lists') :></a>
                </li>
                <li>
                  <a href="http://forums.musicbrainz.org" class="internal">
                    <: l('Forums') :>
                  </a>
                </li>
                <li class="separator">
                  <a href="http://tickets.musicbrainz.org" class="internal">
                    <: l('Report a Bug') :>
                  </a>
                </li>
                <li>
                  <a href="<: doc_link('Copyright_Violation_Notice') :>"><: l('Report a Copyright Violation') :></a>
                </li>
              </ul>
            </li>
          </ul>
          <div class="r"></div><div class="l"></div>
        </div>
      </div>
    </div>

    <div id="page">
      <div id="sidebar">
        : block sidebar -> { }
      </div>
      <div id="content">
        : block content -> { }
      </div>
      <div class="br"></div><div class="bl"></div><div class="tr"></div><div class="tl"></div>
      <div style="clear: both"></div>
    </div>

    <div id="footer">
      <p class="left">
        <a href="http://metabrainz.org/donate" class="internal">Donate</a> |
        <a href="http://wiki.musicbrainz.org/" class="internal">Wiki</a> |
        <a href="http://forums.musicbrainz.org/" class="internal">Forums</a> |
        <a href="http://tickets.musicbrainz.org/" class="internal">Bug Tracker</a> |
        <a href="http://twitter.com/MusicBrainz" class="internal">Twitter</a> |
        <a href="http://www.cafepress.com/musicbrainz" class="internal">Schwag</a>
      </p>

      <p class="right">
        Hosted by <a href="http://digitalwest.net/">Digital West</a>. Sponsored by: <a href="http://www.google.com/">Google</a>,
        <a href="http://osuosl.org/">OSUOSL</a> and <a href="/doc/MusicBrainz_Sponsors">othersâ€¦</a>
      </p>
    </div>
  </body>
</html>
